#include<bits/stdc++.h>
#define ll long long
using namespace std;
int cnt=0;
void rec(ll n)
{
    n/=2;
    if(n==0)
    {
        cout<<cnt<<endl;
        return;
    }
    cnt++;
    return rec(n);
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    ll n;
    cin>>n;
    rec(n);
return 0;
}
