#include <bits/stdc++.h>
using namespace std;
///stol(string)---> string to int
///to_string(int)--->int to string
int rec(int x)
{
    if(x!=0)
    {
        if(x==1) cout<<x;
        else cout<<x<<" ";
    }
    if(x==0) return x;
    return rec(x-1);
}
int main()
{
    int x;
    cin>>x;
    rec(x);
    return 0;
}
