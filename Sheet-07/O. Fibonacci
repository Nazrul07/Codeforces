#include<bits/stdc++.h>
#define ll long long
using namespace std;
ll rec(ll n)
{
    if(n==1) return 0;
    if(n==2) return 1;
    return rec(n-1)+rec(n-2);
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    ll n,res;
    cin>>n;
    res=rec(n);
    cout<<res<<endl;
return 0;
}
