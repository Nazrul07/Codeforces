#include <bits/stdc++.h>
using namespace std;
int index=0;
void rec(string x)
{
    if(index==x.size()) return;
    cout<<x[index]<<" ";
    index++;
    return rec(x);
}
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        string s;
        cin>>s;
        rec(s);
        index=0;
        cout << endl;
    }
    return 0;
}
